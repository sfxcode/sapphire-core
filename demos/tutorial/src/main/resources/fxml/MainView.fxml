<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" styleClass="workspace"
           minWidth="700" xmlns="http://javafx.com/javafx/11.0.1"
           fx:controller="com.sfxcode.sapphire.core.demo.tutorial.controller.MainViewController">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <AnchorPane fx:id="navigationPane"
                            prefHeight="40.0" styleClass="navigation"/>
            </top>
            <center>
                <AnchorPane fx:id="workspacePane" minHeight="450"/>
            </center>
            <bottom>
                <AnchorPane fx:id="statusPane" styleClass="status"/>
            </bottom>
        </BorderPane>
    </children>
    <stylesheets>
        <URL value="@default.css"/>
    </stylesheets>
</StackPane>
