<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import java.net.URL?>
<StackPane prefHeight="600.0" prefWidth="200.0" styleClass="main"
           xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.sfxcode.sapphire.core.demo.windows.controller.AdditionalViewController">
    <stylesheets>
        <URL value="@default.css" />
    </stylesheets>
   <children>
      <BorderPane>
         <center>
            <Label fx:id="windowLabel" styleClass="smallInfoText" text="" BorderPane.alignment="CENTER">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></Label>
         </center>
      </BorderPane>
   </children>
</StackPane>
